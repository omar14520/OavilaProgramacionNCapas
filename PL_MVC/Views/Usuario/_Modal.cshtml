<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
<script src="http://bootboxjs.com/bootbox.js"></script>
<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
<style>
    .table td, .table th {
        word-wrap: break-word;
        white-space: normal;
        max-width: 200px;
        overflow: hidden;
        text-overflow: ellipsis;
    }
</style>
<div class="modal" tabindex="-1" role="dialog" id="Modal">
    <div class="modal-dialog modal-sm" style="width: 50%" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Aviso</h4>
            </div>
            <div class="modal-body">
                @if (ViewBag.ErroresExcel != null)
                {
                    if (ViewBag.ErroresExcel is IEnumerable<object>)
                    {
                        foreach (var error in ViewBag.ErroresExcel)
                        {
                            <br />
                            <h5 class="col-1">
                                @error.ErrorMessage
                            </h5>
                        }
                    }
                    else
                    {
                        <br />
                        <h5 class="col-1">
                            @ViewBag.ErroresExcel
                        </h5>
                    }
                }

                @if ((ViewBag.usuariosCorrectos != null && ViewBag.usuariosCorrectos.Count > 0) || (ViewBag.usuariosIncorrectos != null && ViewBag.usuariosIncorrectos.Count > 0))
                {
                    <h5>Los usuarios insertados correctamente fueron: @ViewBag.usuariosCorrectos.Count</h5>
                    <h5>Los usuarios no insertados fueron: @ViewBag.usuariosIncorrectos.Count</h5>
                    if (ViewBag.usuariosCorrectos != null && ViewBag.usuariosCorrectos.Count > 0)
                    {
                        <div>
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th scope="col">Usuario</th>
                                        <th scope="col">Nombre</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        @foreach (ML.Usuario usuarios in (List<ML.Usuario>)ViewBag.usuariosCorrectos)
                                        {

                                            <th>@usuarios.UserName</th>
                                            <th>@usuarios.Nombre</th>

                                        }
                                    </tr>
                                </tbody>
                            </table>
                        </div>

                        if (ViewBag.usuariosIncorrectos != null && ViewBag.usuariosIncorrectos.Count > 0)
                        {
                            <div>
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th scope="col">Usuario</th>
                                            <th scope="col">Nombre</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            @foreach (ML.Usuario usuarios in (List<ML.Usuario>)ViewBag.usuariosIncorrectos)
                                            {

                                                <th>@usuarios.UserName</th>
                                                <th>@usuarios.Nombre</th>

                                            }
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        }
                    }
                }
            </div>
            <div class="modal-footer">
                @Html.ActionLink("Continuar", "GetAll", "Usuario", new { area = "" }, htmlAttributes: new { @class = "btn btn-danger" })
            </div>
        </div>
    </div>
</div>
<script>$('#Modal').modal();</script>t>